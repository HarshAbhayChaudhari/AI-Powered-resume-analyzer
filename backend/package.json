{
  "name": "resume-analyzer-backend",
  "version": "1.0.0",
  "description": "Backend API for AI-powered resume analyzer",
  "main": "index.js",
  "packageManager": "bun@1.2.10",
  "scripts": {
    "start": "bunx functions-framework --target=analyzeResume",
    "deploy": "gcloud functions deploy analyzeResume --runtime nodejs18 --trigger-http --allow-unauthenticated",
    "test": "bunx jest",
    "dev": "bunx functions-framework --target=analyzeResume",
    "install": "bun install"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "pdf-parse": "^1.1.1",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "firebase-admin": "^11.11.0",
    "openai": "^4.20.0",
    "natural": "^6.10.4",
    "compromise": "^14.10.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "engines": {
    "node": "18"
  }
} 